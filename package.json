{
  "name": "page-saver-extension",
  "version": "1.0.0",
  "description": "Chrome extension to save web pages with keyboard shortcuts and floating button",
  "scripts": {
    "generate-icons": "npx ts-node scripts/generate-icons.ts",
    "prebuild": "pnpm run generate-icons",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "build:tailwind:popup": "npx tailwindcss -i ./src/popup/popup.css -o ./src/popup/popup.output.css --minify"
    
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv-webpack": "^8.1.1",
    "express": "^4.21.2",
    "lucide": "^0.474.0",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.x",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^9.x.x",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.x.x",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.x.x",
    "webpack": "^5.x.x",
    "webpack-cli": "^4.x.x"
  }
}